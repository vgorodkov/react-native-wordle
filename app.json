{
  "expo": {
    "name": "beWordle",
    "slug": "wordle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "backgroundColor": "#000000",
    "scheme": "your-app-scheme",
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tragediabelok.wordle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A1A1A"
      },
      "package": "com.tragediabelok.wordle"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bb142e57-8d82-45ab-b33d-a763abc8f959"
      }
    }
  }
}
