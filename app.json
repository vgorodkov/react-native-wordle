{
  "expo": {
    "name": "beWordle",
    "slug": "beWordle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "backgroundColor": "#000000",
    "scheme": "your-app-scheme",
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground-image.png",
        "backgroundImage": "./assets/background-image.png",
        "backgroundColor": "#000000"
      },
      "package": "com.tragediabelok.wordle"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bb142e57-8d82-45ab-b33d-a763abc8f959"
      }
    }
  }
}
